<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<managed-bean>
		<managed-bean-name>usuarioMB</managed-bean-name>
		<managed-bean-class>br.com.youteacher.manegedbean.UsuarioMB</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>viewBean</property-name>
			<property-class>br.com.youteacher.viewbean.UsuarioViewBean</property-class>
			<value />
		</managed-property>
	</managed-bean>
	<application>
		<resource-bundle>
			<base-name>/messages</base-name>
			<var>messages</var>
		</resource-bundle>
	</application>

	<!-- Regras de navegações da pagina -->
	<navigation-rule>
		<from-view-id>/pages/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logado</from-outcome>
			<to-view-id>/aluno/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/adm.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>logado2</from-outcome>
			<to-view-id>/adm/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/aluno/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sair</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>video</from-outcome>
			<to-view-id>/aluno/addvideo.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/aluno/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/aluno/perfil.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
			<navigation-case>
			<from-outcome>praticar</from-outcome>
			<to-view-id>/aluno/praticar.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>formulario</from-outcome>
			<to-view-id>/aluno/addformulario.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<!-- Navegação pagina do video -->
	<navigation-rule>
		<from-view-id>/aluno/addvideo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sair</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/aluno/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>formulario</from-outcome>
			<to-view-id>/aluno/addformulario.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/aluno/perfil.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<!-- Navegação pagina do formulário -->
	<navigation-rule>
		<from-view-id>/aluno/addformulario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sair</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/aluno/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/aluno/perfil.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<!-- Navegação pagina do Perfil -->
	<navigation-rule>
		<from-view-id>/aluno/perfil.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sair</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/aluno/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/aluno/perfil.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<!-- Navegação pagina do praticar -->
	<navigation-rule>
		<from-view-id>/aluno/praticar.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sair</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/aluno/home.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>perfil</from-outcome>
			<to-view-id>/aluno/perfil.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	
	
</faces-config>
